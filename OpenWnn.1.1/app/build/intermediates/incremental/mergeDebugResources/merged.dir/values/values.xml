        <!-- Layout resource for key press feedback.
             {@deprecated Copy this definition into your own application project.} -->
        <attr format="reference" name="keyPreviewLayout"/>

        <!-- Vertical offset of the key press feedback from the key.
             {@deprecated Copy this definition into your own application project.} -->
        <attr format="dimension" name="keyPreviewOffset"/>

        <!-- Height of the key press feedback popup.
             {@deprecated Copy this definition into your own application project.} -->
        <attr format="dimension" name="keyPreviewHeight"/>

        <!-- Amount to offset the touch Y coordinate by, for bias correction.
             {@deprecated Copy this definition into your own application project.} -->
        <attr format="dimension" name="verticalCorrection"/>

        <!-- Layout resource for popup keyboards.
             {@deprecated Copy this definition into your own application project.} -->
